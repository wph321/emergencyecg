      <compilerarg line="${compiler.args.emergencyecg}"/>
      <bootclasspath refid="emergencyecg.module.bootclasspath"/>
      <classpath refid="emergencyecg.module.production.classpath"/>
      <src refid="emergencyecg.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.emergencyecg"/>
    </javac2>
    
    <copy todir="${emergencyecg.output.dir}">
      <fileset dir="${module.emergencyecg.basedir}/build/generated/source/apt/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.emergencyecg.basedir}/build/generated/source/r/debug">
